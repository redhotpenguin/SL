sl_root  /home/fred/dev/sl/trunk/SL
sl_httpd_root  /home/fred/dev/sl/httpd2
sl_version  trunk

sl_data_root /tmp/data/sl
sl_ip_cache_file	/tmp/sl_ips

# configure the database connection
sl_db_name  sl2
sl_db_host	localhost
sl_db_user  sl
