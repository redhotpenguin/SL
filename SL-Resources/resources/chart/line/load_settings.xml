<settings> 
  <data_type>CSV</data_type>                                   
  <font>Arial</font>                                          
  <text_size>9</text_size>                                    
  <thousands_separator>,</thousands_separator>
  <decimals_separator>.</decimals_separator>
  <csv_separator>,</csv_separator>
  <plot_area>
    <margins>
      <top>15</top>
    </margins>
  </plot_area>

  <grid>
    <x>                                                      
      <alpha>5</alpha>
    </x>
    <y_left>      
      <alpha>5</alpha>
      <approx_count>5</approx_count>
    </y_left>
 </grid>
  
  <values>                 
    <x>                    
      <skip_first>false</skip_first>
      <skip_last>false</skip_last>  
      <color>#666666</color>        
      <text_size>9</text_size>
      <rotate>90</rotate>
      <frequency>1</frequency>
    </x>

    <y_left>
      <skip_first>false</skip_first>
      <frequency>1</frequency>
      <unit> ms</unit>
      <integers_only>true</integers_only>
    </y_left>
  </values>


  
  <axes>                           
    <x>                            
      <color>#666666</color>       
      <alpha>100</alpha>   
      <width>1</width>     
    </x>
    <y_left>
      <alpha>0</alpha>
    </y_left>
 </axes>  
  
  <indicator>
    <color>D84B3C</color>
    <selection_color>D84B3C</selection_color>
    <line_alpha>100</line_alpha>
    <x_balloon_text_color>000000</x_balloon_text_color> 
  </indicator>
  
  <vertical_lines>
   <width>80</width>
   <alpha>70</alpha>
  </vertical_lines>
  
  
  <graphs>             
    <graph gid="1">
      <axis>left</axis>
      <title>System Load</title>
      <color>#9ce4ff</color>   
      <balloon_text_color>#333333</balloon_text_color>
      <balloon_text>
        <![CDATA[{value}]]>
      </balloon_text>
      <fill_alpha>80</fill_alpha>
    </graph>
   
   <balloon_text_color>#333333</balloon_text_color>                     
      <balloon_text>
        <![CDATA[{value}]]>
      </balloon_text>
      <fill_alpha>20</fill_alpha>
    </graph>

  </graphs>  
</settings>
