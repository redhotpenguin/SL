<settings> 
  <data_type>CSV</data_type>                                   
  <font>Tahoma</font>                                          
  <text_size>11</text_size>                                    
  <thousands_separator>,</thousands_separator>
  <csv_separator>,</csv_separator>
  <plot_area>
    <margins>
      <top>15</top>
    </margins>
  </plot_area>

  <grid>
    <x>                                                      
      <alpha>10</alpha>
    </x>
    <y_left>      
      <alpha>15</alpha>
    </y_left>
    <y_right>      
      <alpha>30</alpha>
    </y_right>    
  </grid>
  
  <values>                 
    <x>                    
      <skip_first>false</skip_first>
      <skip_last>false</skip_last>  
      <color>999999</color>        
      <text_size>10</text_size>
      <rotate>90</rotate>
      <frequency>1</frequency>
    </x>
    <y_left>
      <unit> MB</unit>
    </y_left>
  </values>
  
  <axes>                           
    <x>                            
      <color>E7E7E7</color>       
      <alpha>100</alpha>   
      <width>1</width>     
    </x>
    <y_left>
      <alpha>0</alpha>
    </y_left>
    <y_right>
      <alpha>0</alpha>
    </y_right>    
  </axes>  
  
  <indicator>
    <color>D84B3C</color>
    <selection_color>D84B3C</selection_color>
    <line_alpha>50</line_alpha>
    <x_balloon_text_color>000000</x_balloon_text_color> 
  </indicator>
  
  <vertical_lines>
   <width>80</width>
   <alpha>70</alpha>
  </vertical_lines>
  
  
  <graphs>             
    <graph gid="1">
      <axis>left</axis>
      <title>Megabytes Downloaded</title>
      <color>#FAC314</color>   
      <balloon_text_color>000000</balloon_text_color>
      <balloon_text>
        <![CDATA[{value} MB downloaded]]>
      </balloon_text>
      <fill_alpha>50</fill_alpha>
    </graph>
    
    <graph gid="2">
      <axis>left</axis>
      <title>Megabytes Uploaded</title>              
      <color>#D84B3C</color>
      <balloon_text_color>000000</balloon_text_color>                     
      <balloon_text>
        <![CDATA[{value} MB uploaded]]>
      </balloon_text>
      <fill_alpha>50</fill_alpha>
    </graph>

    <graph gid="3">
      <axis>right</axis>
      <title>Unique Users</title>              
      <color>#D84B3C</color>
      <balloon_text_color>000000</balloon_text_color>                     
      <balloon_text>
        <![CDATA[{value} users]]>
      </balloon_text>
      <fill_alpha>50</fill_alpha>
    </graph>    


  </graphs>  
</settings>
