[% PROCESS header.tmpl title="Silver Lining Dashboard:  Manage Routers" selected="network" header=head load="html_googlemaps_initialize()" %]

<div id="main_content" class="grid_16">	


	[% PROCESS router/tertiarynav.tmpl active="overview" %]

    
	<div class="grid_6 alpha">
		 <ul>
		     <li>There are [% active_nodes || 0 %] routers in this network</li>
		     <li>[% problem_nodes || 0 %] routers have had recent problems</li>
		     <li>[% inactive_nodes || 0 %] routers are inactive</li>
		     <li>[% account.users_lastmonth %] users transferred [% account.megabytes_lastmonth %] megabytes in the last 30 days</li>
		 </ul>
	</div>



    <div class="grid_10 omega">

<!-- amxy script-->
<script type="text/javascript" src="[% base_uri %]/resources/amxy/swfobject.js"></script>
        <div id="flashcontent">
                <strong>You need to upgrade your Flash Player</strong>
        </div>

        <script type="text/javascript">
                // <![CDATA[            
                var so = new SWFObject("[% base_uri %]/resources/amxy/amxy.swf","amxy", "700px", "400px", "4", "#FFFFFF");
                so.addVariable("path", "[% base_uri %]/resources/amxy/");
                so.addVariable("settings_file", encodeURIComponent("[% base_uri %]/resources/amxy/amxy_settings.xml"));
                so.addVariable("data_file", encodeURIComponent("[% base_uri %]/resources/amxy/amxy_data.xml"))
;
                so.write("flashcontent");
                // ]]>
        </script>
<!-- end of amxy script -->

</div>

<!--
    <div class="grid_10 omega">


		<img src="[% base_uri %]/resources/images/graphs/mrtg.png">
	</div>
-->	
<div class="clear"></div>


      [% IF map_error %]
	  <div class="grid_6 alpha">

      <h3><span class="error">Mapping services are currently unavailable</span></h3>
	
      </div>
      [% ELSE %]


	<div id="map" class="alpha omega grid_16">
	
    [% map %]
			
	</div>
    [% END %]

	<div class="clear"></div>
	
</div>





[% PROCESS footer.tmpl %]
