#!perl -w

use strict;
use warnings;

use SL::Client::HTTP;

my $url = shift or die "No url passed!\n";
my $port = shift or die "No port passed!\n";
my $host = shift or die "using default host\n";

my $response = SL::Client::HTTP->get(
	url => $url,
	host => $host,
	port => $port,
);

use Data::Dumper;
print Dumper($response);

sleep 1;
